     50          extern uint8_t Getchar(void)        //接收数据
     51          {
     52          	uint8_t c;
     53          	unsigned int	i,delay;
     54          	static unsigned int timeout;
     55          	while(1)
     56          	{
     57          		event_proc();
     58          
     59          		if (current_channel == -1)
     60          		{
     61          			//还未进入打印通道的打印会话过程
     62          			for (i = 0; i < MAX_PRINT_CHANNEL;i++)
     63          			{
     64          				if (ringbuffer_getchar(&spp_ringbuf[i],&c))
     65          				{
     66          					current_channel = i;
     67          #ifdef DEBUG_VER
     68          					//debug_buffer[0] = c;
     69          					//debug_cnt = 1;
     70          #endif
     71          					return c;
     72          				}
     73          			}
     74          		}
     75          		else
     76          		{
     77          			//已经进入某一个打印通道的打印会话过程
     78          			if (ringbuffer_getchar(&spp_ringbuf[current_channel],&c))
     79          			{
     80          				timeout = 0;
     81          				if (ringbuffer_data_len(&spp_ringbuf[current_channel]) < RING_BUFF_EMPTY_TH)
     82          				{
     83          					if (current_channel != USB_PRINT_CHANNEL_OFFSET)
     84          					{
     85          						set_BT_FREE(current_channel);
     86          					}
     87          					else
     88          					{
     89          						SetEPRxStatus(EP2_OUT, EP_RX_VALID);
     90          					}
     91          				}
     92          #ifdef DEBUG_VER
     93          				//debug_buffer[debug_cnt] = c;
     94          				//debug_cnt++;
     95          #endif
     96          				return c;
     97          			}
     98          			else
     99          			{
    100          				delay = SysTick_GetCounter();
    101          				if (timeout == 0)
    102          				{
    103          					timeout = delay;
    104          				}
    105          				else
    106          				{
    107          					if (delay > timeout)
    108          					{
    109          						delay = delay-timeout;
    110          					}
    111          					else
    112          					{
    113          						delay += 0xffffffff-timeout;
    114          					}
    115          
    116          					if (delay > (CHANNEL_TIMEOUT_TH/10))	//systick是10ms计数的
    117          					{
    118          						PrintBufToZero();
    119          						current_channel = -1;
    120          						//debug_cnt = 0;
    121          					}
    122          				}
    123          			}
    124          		}
    125          		
    126          	}
    127          
    128          }
