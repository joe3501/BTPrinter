     52          extern uint8_t Getchar(void)        //接收数据
     53          {
     54          	uint8_t c;
     55          	unsigned int	i,delay;
     56          	static unsigned int timeout;
     57          	while(1)
     58          	{
     59          		event_proc();
     60          
     61          		if (current_channel == -1)
     62          		{
     63          			//还未进入打印通道的打印会话过程
     64          			for (i = 0; i < MAX_PRINT_CHANNEL;i++)
     65          			{
     66          				if (ringbuffer_getchar(&spp_ringbuf[i],&c))
     67          				{
     68          					timeout = 0;
     69          					//debug_cnt = 0;
     70          					current_channel = i;
     71          #ifdef DEBUG_VER
     72          					//debug_buffer[0] = c;
     73          					//debug_cnt = 1;
     74          #endif
     75          					//trip1();
     76          					return c;
     77          				}
     78          			}
     79          		}
     80          		else
     81          		{
     82          			//已经进入某一个打印通道的打印会话过程
     83          			if (ringbuffer_getchar(&spp_ringbuf[current_channel],&c))
     84          			{
     85          				timeout = 0;
     86          				//debug_cnt = 0;
     87          				if (ringbuffer_data_len(&spp_ringbuf[current_channel]) < RING_BUFF_EMPTY_TH)
     88          				{
     89          					if (current_channel != USB_PRINT_CHANNEL_OFFSET)
     90          					{
     91          						set_BT_free(current_channel);
     92          					}
     93          					else
     94          					{
     95          						SetEPRxStatus(EP2_OUT, EP_RX_VALID);
     96          					}
     97          				}
     98          #ifdef DEBUG_VER
     99          				//debug_buffer[debug_cnt] = c;
    100          				//debug_cnt++;
    101          #endif
    102          				//trip1();
    103          				return c;
    104          			}
    105          			else
    106          			{
    107          				delay = systick_cnt;		//systick中断中维持的递增计数器
    108          				if (timeout == 0)
    109          				{
    110          					timeout = (delay!=0)?delay:1;
    111          				}
    112          				else
    113          				{
    114          					if (delay != timeout)
    115          					{
    116          						if (delay > timeout)
    117          						{
    118          							delay = delay - timeout;
    119          						}
    120          						else
    121          						{
    122          							delay += 0xffffffff-timeout;
    123          						}
    124          
    125          						if (delay > (CHANNEL_TIMEOUT_TH/10))	//systick是10ms计数的
    126          						{
    127          							//trip3();
    128          							PrintBufToZero();
    129          							current_channel = -1;
    130          							//debug_cnt = 0;
    131          						}
    132          					}
    133          				}
    134          			}
    135          		}
    136          		
    137          	}
    138          
    139          }
