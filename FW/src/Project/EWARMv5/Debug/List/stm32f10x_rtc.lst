     87          /*******************************************************************************
     88          * Function Name  : RTC_GetCounter
     89          * Description    : Gets the RTC counter value.
     90          * Input          : None
     91          * Output         : None
     92          * Return         : RTC counter value.
     93          *******************************************************************************/
     94          u32 RTC_GetCounter(void)
     95          {
     96            u16 tmp = 0;
     97            tmp = RTC->CNTL;
     98          
     99            return (((u32)RTC->CNTH << 16 ) | tmp) ;
    100          }
