    162          /*******************************************************************************
    163          * Function Name  : RTC_GetDivider
    164          * Description    : Gets the RTC divider value.
    165          * Input          : None
    166          * Output         : None
    167          * Return         : RTC Divider value.
    168          *******************************************************************************/
    169          u32 RTC_GetDivider(void)
    170          {
    171            u32 tmp = 0x00;
    172          
    173            tmp = ((u32)RTC->DIVH & (u32)0x000F) << 16;
    174            tmp |= RTC->DIVL;
    175          
    176            return tmp;
    177          }
