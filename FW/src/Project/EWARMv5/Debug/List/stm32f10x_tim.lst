   2960          /*******************************************************************************
   2961          * Function Name  : TIM_GetITStatus
   2962          * Description    : Checks whether the TIM interrupt has occurred or not.
   2963          * Input          : - TIMx: where x can be 1 to 8 to select the TIM peripheral.
   2964          *                  - TIM_IT: specifies the TIM interrupt source to check.
   2965          *                    This parameter can be one of the following values:
   2966          *                       - TIM_IT_Update: TIM update Interrupt source
   2967          *                       - TIM_IT_CC1: TIM Capture Compare 1 Interrupt source
   2968          *                       - TIM_IT_CC2: TIM Capture Compare 2 Interrupt source
   2969          *                       - TIM_IT_CC3: TIM Capture Compare 3 Interrupt source
   2970          *                       - TIM_IT_CC4: TIM Capture Compare 4 Interrupt source
   2971          *                       - TIM_IT_COM: TIM Commutation Interrupt
   2972          *                         source
   2973          *                       - TIM_IT_Trigger: TIM Trigger Interrupt source
   2974          *                       - TIM_IT_Break: TIM Break Interrupt source
   2975          * Output         : None
   2976          * Return         : The new state of the TIM_IT(SET or RESET).
   2977          *******************************************************************************/
   2978          ITStatus TIM_GetITStatus(TIM_TypeDef* TIMx, u16 TIM_IT)
   2979          {
   2980            ITStatus bitstatus = RESET;  
   2981            u16 itstatus = 0x0, itenable = 0x0;
   2982          
   2983            /* Check the parameters */
   2984            assert_param(IS_TIM_ALL_PERIPH(TIMx));
   2985            assert_param(IS_TIM_GET_IT(TIM_IT));
   2986            assert_param(IS_TIM_PERIPH_IT(TIMx, TIM_IT));
   2987             
   2988            itstatus = TIMx->SR & TIM_IT;
   2989            
   2990            itenable = TIMx->DIER & TIM_IT;
   2991          
   2992            if ((itstatus != (u16)RESET) && (itenable != (u16)RESET))
   2993            {
   2994              bitstatus = SET;
   2995            }
   2996            else
   2997            {
   2998              bitstatus = RESET;
   2999            }
   3000            return bitstatus;
   3001          }
