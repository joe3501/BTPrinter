   3430          /*-----------------------------------------------------------------------*/
   3431          /* Rename File/Directory                                                 */
   3432          /*-----------------------------------------------------------------------*/
   3433          
   3434          FRESULT f_rename (
   3435          	const TCHAR *path_old,	/* Pointer to the old name */
   3436          	const TCHAR *path_new	/* Pointer to the new name */
   3437          )
   3438          {
   3439          	FRESULT res;
   3440          	DIR djo, djn;
   3441          	BYTE buf[21], *dir;
   3442          	DWORD dw;
   3443          	DEF_NAMEBUF;
   3444          
   3445          
   3446          	res = chk_mounted(&path_old, &djo.fs, 1);
   3447          	if (res == FR_OK) {
   3448          		djn.fs = djo.fs;
   3449          		INIT_BUF(djo);
   3450          		res = follow_path(&djo, path_old);		/* Check old object */
   3451          		if (_FS_RPATH && res == FR_OK && (djo.fn[NS] & NS_DOT))
   3452          			res = FR_INVALID_NAME;
   3453          #if _FS_SHARE
   3454          		if (res == FR_OK) res = chk_lock(&djo, 2);
   3455          #endif
   3456          		if (res == FR_OK) {						/* Old object is found */
   3457          			if (!djo.dir) {						/* Is root dir? */
   3458          				res = FR_NO_FILE;
   3459          			} else {
   3460          				mem_cpy(buf, djo.dir+DIR_Attr, 21);		/* Save the object information except for name */
   3461          				mem_cpy(&djn, &djo, sizeof(DIR));		/* Check new object */
   3462          				res = follow_path(&djn, path_new);
   3463          				if (res == FR_OK) res = FR_EXIST;		/* The new object name is already existing */
   3464          				if (res == FR_NO_FILE) { 				/* Is it a valid path and no name collision? */
   3465          /* Start critical section that any interruption or error can cause cross-link */
   3466          					res = dir_register(&djn);			/* Register the new entry */
   3467          					if (res == FR_OK) {
   3468          						dir = djn.dir;					/* Copy object information except for name */
   3469          						mem_cpy(dir+13, buf+2, 19);
   3470          						dir[DIR_Attr] = buf[0] | AM_ARC;
   3471          						djo.fs->wflag = 1;
   3472          						if (djo.sclust != djn.sclust && (dir[DIR_Attr] & AM_DIR)) {		/* Update .. entry in the directory if needed */
   3473          							dw = clust2sect(djn.fs, LD_CLUST(dir));
   3474          							if (!dw) {
   3475          								res = FR_INT_ERR;
   3476          							} else {
   3477          								res = move_window(djn.fs, dw);
   3478          								dir = djn.fs->win+SZ_DIR;	/* .. entry */
   3479          								if (res == FR_OK && dir[1] == '.') {
   3480          									dw = (djn.fs->fs_type == FS_FAT32 && djn.sclust == djn.fs->dirbase) ? 0 : djn.sclust;
   3481          									ST_CLUST(dir, dw);
   3482          									djn.fs->wflag = 1;
   3483          								}
   3484          							}
   3485          						}
   3486          						if (res == FR_OK) {
   3487          							res = dir_remove(&djo);		/* Remove old entry */
   3488          							if (res == FR_OK)
   3489          								res = sync(djo.fs);
   3490          						}
   3491          					}
   3492          /* End critical section */
   3493          				}
   3494          			}
   3495          		}
   3496          		FREE_BUF();
   3497          	}
   3498          	LEAVE_FF(djo.fs, res);
   3499          }
