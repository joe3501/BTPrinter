   3206          /*-----------------------------------------------------------------------*/
   3207          /* Delete a File or Directory                                            */
   3208          /*-----------------------------------------------------------------------*/
   3209          
   3210          FRESULT f_unlink (
   3211          	const TCHAR *path		/* Pointer to the file or directory path */
   3212          )
   3213          {
   3214          	FRESULT res;
   3215          	DIR dj, sdj;
   3216          	BYTE *dir;
   3217          	DWORD dclst;
   3218          	DEF_NAMEBUF;
   3219          
   3220          
   3221          	res = chk_mounted(&path, &dj.fs, 1);
   3222          	if (res == FR_OK) {
   3223          		INIT_BUF(dj);
   3224          		res = follow_path(&dj, path);		/* Follow the file path */
   3225          		if (_FS_RPATH && res == FR_OK && (dj.fn[NS] & NS_DOT))
   3226          			res = FR_INVALID_NAME;			/* Cannot remove dot entry */
   3227          #if _FS_SHARE
   3228          		if (res == FR_OK) res = chk_lock(&dj, 2);	/* Cannot remove open file */
   3229          #endif
   3230          		if (res == FR_OK) {					/* The object is accessible */
   3231          			dir = dj.dir;
   3232          			if (!dir) {
   3233          				res = FR_INVALID_NAME;		/* Cannot remove the start directory */
   3234          			} else {
   3235          				if (dir[DIR_Attr] & AM_RDO)
   3236          					res = FR_DENIED;		/* Cannot remove R/O object */
   3237          			}
   3238          			dclst = LD_CLUST(dir);
   3239          			if (res == FR_OK && (dir[DIR_Attr] & AM_DIR)) {	/* Is it a sub-dir? */
   3240          				if (dclst < 2) {
   3241          					res = FR_INT_ERR;
   3242          				} else {
   3243          					mem_cpy(&sdj, &dj, sizeof(DIR));	/* Check if the sub-dir is empty or not */
   3244          					sdj.sclust = dclst;
   3245          					res = dir_sdi(&sdj, 2);		/* Exclude dot entries */
   3246          					if (res == FR_OK) {
   3247          						res = dir_read(&sdj);
   3248          						if (res == FR_OK			/* Not empty dir */
   3249          #if _FS_RPATH
   3250          						|| dclst == sdj.fs->cdir	/* Current dir */
   3251          #endif
   3252          						) res = FR_DENIED;
   3253          						if (res == FR_NO_FILE) res = FR_OK;	/* Empty */
   3254          					}
   3255          				}
   3256          			}
   3257          			if (res == FR_OK) {
   3258          				res = dir_remove(&dj);		/* Remove the directory entry */
   3259          				if (res == FR_OK) {
   3260          					if (dclst)				/* Remove the cluster chain if exist */
   3261          						res = remove_chain(dj.fs, dclst);
   3262          					if (res == FR_OK) res = sync(dj.fs);
   3263          				}
   3264          			}
   3265          		}
   3266          		FREE_BUF();
   3267          	}
   3268          	LEAVE_FF(dj.fs, res);
   3269          }
