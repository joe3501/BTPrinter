   3274          /*-----------------------------------------------------------------------*/
   3275          /* Create a Directory                                                    */
   3276          /*-----------------------------------------------------------------------*/
   3277          
   3278          FRESULT f_mkdir (
   3279          	const TCHAR *path		/* Pointer to the directory path */
   3280          )
   3281          {
   3282          	FRESULT res;
   3283          	DIR dj;
   3284          	BYTE *dir, n;
   3285          	DWORD dsc, dcl, pcl, tim = get_fattime();
   3286          	DEF_NAMEBUF;
   3287          
   3288          
   3289          	res = chk_mounted(&path, &dj.fs, 1);
   3290          	if (res == FR_OK) {
   3291          		INIT_BUF(dj);
   3292          		res = follow_path(&dj, path);			/* Follow the file path */
   3293          		if (res == FR_OK) res = FR_EXIST;		/* Any object with same name is already existing */
   3294          		if (_FS_RPATH && res == FR_NO_FILE && (dj.fn[NS] & NS_DOT))
   3295          			res = FR_INVALID_NAME;
   3296          		if (res == FR_NO_FILE) {				/* Can create a new directory */
   3297          			dcl = create_chain(dj.fs, 0);		/* Allocate a cluster for the new directory table */
   3298          			res = FR_OK;
   3299          			if (dcl == 0) res = FR_DENIED;		/* No space to allocate a new cluster */
   3300          			if (dcl == 1) res = FR_INT_ERR;
   3301          			if (dcl == 0xFFFFFFFF) res = FR_DISK_ERR;
   3302          			if (res == FR_OK)					/* Flush FAT */
   3303          				res = move_window(dj.fs, 0);
   3304          			if (res == FR_OK) {					/* Initialize the new directory table */
   3305          				dsc = clust2sect(dj.fs, dcl);
   3306          				dir = dj.fs->win;
   3307          				mem_set(dir, 0, SS(dj.fs));
   3308          				mem_set(dir+DIR_Name, ' ', 8+3);	/* Create "." entry */
   3309          				dir[DIR_Name] = '.';
   3310          				dir[DIR_Attr] = AM_DIR;
   3311          				ST_DWORD(dir+DIR_WrtTime, tim);
   3312          				ST_CLUST(dir, dcl);
   3313          				mem_cpy(dir+SZ_DIR, dir, SZ_DIR); 	/* Create ".." entry */
   3314          				dir[33] = '.'; pcl = dj.sclust;
   3315          				if (dj.fs->fs_type == FS_FAT32 && pcl == dj.fs->dirbase)
   3316          					pcl = 0;
   3317          				ST_CLUST(dir+SZ_DIR, pcl);
   3318          				for (n = dj.fs->csize; n; n--) {	/* Write dot entries and clear following sectors */
   3319          					dj.fs->winsect = dsc++;
   3320          					dj.fs->wflag = 1;
   3321          					res = move_window(dj.fs, 0);
   3322          					if (res != FR_OK) break;
   3323          					mem_set(dir, 0, SS(dj.fs));
   3324          				}
   3325          			}
   3326          			if (res == FR_OK) res = dir_register(&dj);	/* Register the object to the directoy */
   3327          			if (res != FR_OK) {
   3328          				remove_chain(dj.fs, dcl);			/* Could not register, remove cluster chain */
   3329          			} else {
   3330          				dir = dj.dir;
   3331          				dir[DIR_Attr] = AM_DIR;				/* Attribute */
   3332          				ST_DWORD(dir+DIR_WrtTime, tim);		/* Created time */
   3333          				ST_CLUST(dir, dcl);					/* Table start cluster */
   3334          				dj.fs->wflag = 1;
   3335          				res = sync(dj.fs);
   3336          			}
   3337          		}
   3338          		FREE_BUF();
   3339          	}
   3340          
   3341          	LEAVE_FF(dj.fs, res);
   3342          }
