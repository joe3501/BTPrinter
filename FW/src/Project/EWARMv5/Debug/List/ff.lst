   3349          /*-----------------------------------------------------------------------*/
   3350          
   3351          FRESULT f_chmod (
   3352          	const TCHAR *path,	/* Pointer to the file path */
   3353          	BYTE value,			/* Attribute bits */
   3354          	BYTE mask			/* Attribute mask to change */
   3355          )
   3356          {
   3357          	FRESULT res;
   3358          	DIR dj;
   3359          	BYTE *dir;
   3360          	DEF_NAMEBUF;
   3361          
   3362          
   3363          	res = chk_mounted(&path, &dj.fs, 1);
   3364          	if (res == FR_OK) {
   3365          		INIT_BUF(dj);
   3366          		res = follow_path(&dj, path);		/* Follow the file path */
   3367          		FREE_BUF();
   3368          		if (_FS_RPATH && res == FR_OK && (dj.fn[NS] & NS_DOT))
   3369          			res = FR_INVALID_NAME;
   3370          		if (res == FR_OK) {
   3371          			dir = dj.dir;
   3372          			if (!dir) {						/* Is it a root directory? */
   3373          				res = FR_INVALID_NAME;
   3374          			} else {						/* File or sub directory */
   3375          				mask &= AM_RDO|AM_HID|AM_SYS|AM_ARC;	/* Valid attribute mask */
   3376          				dir[DIR_Attr] = (value & mask) | (dir[DIR_Attr] & (BYTE)~mask);	/* Apply attribute change */
   3377          				dj.fs->wflag = 1;
   3378          				res = sync(dj.fs);
   3379          			}
   3380          		}
   3381          	}
   3382          
   3383          	LEAVE_FF(dj.fs, res);
   3384          }
