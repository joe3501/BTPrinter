   3093          /*-----------------------------------------------------------------------*/
   3094          /* Get Number of Free Clusters                                           */
   3095          /*-----------------------------------------------------------------------*/
   3096          
   3097          FRESULT f_getfree (
   3098          	const TCHAR *path,	/* Pointer to the logical drive number (root dir) */
   3099          	DWORD *nclst,		/* Pointer to the variable to return number of free clusters */
   3100          	FATFS **fatfs		/* Pointer to pointer to corresponding file system object to return */
   3101          )
   3102          {
   3103          	FRESULT res;
   3104          	DWORD n, clst, sect, stat;
   3105          	UINT i;
   3106          	BYTE fat, *p;
   3107          
   3108          
   3109          	/* Get drive number */
   3110          	res = chk_mounted(&path, fatfs, 0);
   3111          	if (res == FR_OK) {
   3112          		/* If free_clust is valid, return it without full cluster scan */
   3113          		if ((*fatfs)->free_clust <= (*fatfs)->n_fatent - 2) {
   3114          			*nclst = (*fatfs)->free_clust;
   3115          		} else {
   3116          			/* Get number of free clusters */
   3117          			fat = (*fatfs)->fs_type;
   3118          			n = 0;
   3119          			if (fat == FS_FAT12) {
   3120          				clst = 2;
   3121          				do {
   3122          					stat = get_fat(*fatfs, clst);
   3123          					if (stat == 0xFFFFFFFF) { res = FR_DISK_ERR; break; }
   3124          					if (stat == 1) { res = FR_INT_ERR; break; }
   3125          					if (stat == 0) n++;
   3126          				} while (++clst < (*fatfs)->n_fatent);
   3127          			} else {
   3128          				clst = (*fatfs)->n_fatent;
   3129          				sect = (*fatfs)->fatbase;
   3130          				i = 0; p = 0;
   3131          				do {
   3132          					if (!i) {
   3133          						res = move_window(*fatfs, sect++);
   3134          						if (res != FR_OK) break;
   3135          						p = (*fatfs)->win;
   3136          						i = SS(*fatfs);
   3137          					}
   3138          					if (fat == FS_FAT16) {
   3139          						if (LD_WORD(p) == 0) n++;
   3140          						p += 2; i -= 2;
   3141          					} else {
   3142          						if ((LD_DWORD(p) & 0x0FFFFFFF) == 0) n++;
   3143          						p += 4; i -= 4;
   3144          					}
   3145          				} while (--clst);
   3146          			}
   3147          			(*fatfs)->free_clust = n;
   3148          			if (fat == FS_FAT32) (*fatfs)->fsi_flag = 1;
   3149          			*nclst = n;
   3150          		}
   3151          	}
   3152          	LEAVE_FF(*fatfs, res);
   3153          }
