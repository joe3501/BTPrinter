   1328          /*-----------------------------------------------------------------------*/
   1329          /* Calculate sum of an SFN                                               */
   1330          /*-----------------------------------------------------------------------*/
   1331          #if _USE_LFN
   1332          static
   1333          BYTE sum_sfn (
   1334          	const BYTE *dir		/* Ptr to directory entry */
   1335          )
   1336          {
   1337          	BYTE sum = 0;
   1338          	UINT n = 11;
   1339          
   1340          	do sum = (sum >> 1) + (sum << 7) + *dir++; while (--n);
   1341          	return sum;
   1342          }
   1343          #endif
