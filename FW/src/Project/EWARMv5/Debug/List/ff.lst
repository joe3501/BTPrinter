   3158          /*-----------------------------------------------------------------------*/
   3159          /* Truncate File                                                         */
   3160          /*-----------------------------------------------------------------------*/
   3161          
   3162          FRESULT f_truncate (
   3163          	FIL *fp		/* Pointer to the file object */
   3164          )
   3165          {
   3166          	FRESULT res;
   3167          	DWORD ncl;
   3168          
   3169          
   3170          	res = validate(fp->fs, fp->id);		/* Check validity of the object */
   3171          	if (res == FR_OK) {
   3172          		if (fp->flag & FA__ERROR) {			/* Check abort flag */
   3173          			res = FR_INT_ERR;
   3174          		} else {
   3175          			if (!(fp->flag & FA_WRITE))		/* Check access mode */
   3176          				res = FR_DENIED;
   3177          		}
   3178          	}
   3179          	if (res == FR_OK) {
   3180          		if (fp->fsize > fp->fptr) {
   3181          			fp->fsize = fp->fptr;	/* Set file size to current R/W point */
   3182          			fp->flag |= FA__WRITTEN;
   3183          			if (fp->fptr == 0) {	/* When set file size to zero, remove entire cluster chain */
   3184          				res = remove_chain(fp->fs, fp->sclust);
   3185          				fp->sclust = 0;
   3186          			} else {				/* When truncate a part of the file, remove remaining clusters */
   3187          				ncl = get_fat(fp->fs, fp->clust);
   3188          				res = FR_OK;
   3189          				if (ncl == 0xFFFFFFFF) res = FR_DISK_ERR;
   3190          				if (ncl == 1) res = FR_INT_ERR;
   3191          				if (res == FR_OK && ncl < fp->fs->n_fatent) {
   3192          					res = put_fat(fp->fs, fp->clust, 0x0FFFFFFF);
   3193          					if (res == FR_OK) res = remove_chain(fp->fs, ncl);
   3194          				}
   3195          			}
   3196          		}
   3197          		if (res != FR_OK) fp->flag |= FA__ERROR;
   3198          	}
   3199          
   3200          	LEAVE_FF(fp->fs, res);
   3201          }
