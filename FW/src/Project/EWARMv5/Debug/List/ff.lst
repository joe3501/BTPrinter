   3508          /*-----------------------------------------------------------------------*/
   3509          /* Forward data to the stream directly (available on only tiny cfg)      */
   3510          /*-----------------------------------------------------------------------*/
   3511          #if _USE_FORWARD && _FS_TINY
   3512          
   3513          FRESULT f_forward (
   3514          	FIL *fp, 						/* Pointer to the file object */
   3515          	UINT (*func)(const BYTE*,UINT),	/* Pointer to the streaming function */
   3516          	UINT btr,						/* Number of bytes to forward */
   3517          	UINT *bf						/* Pointer to number of bytes forwarded */
   3518          )
   3519          {
   3520          	FRESULT res;
   3521          	DWORD remain, clst, sect;
   3522          	UINT rcnt;
   3523          	BYTE csect;
   3524          
   3525          
   3526          	*bf = 0;	/* Initialize byte counter */
   3527          
   3528          	res = validate(fp->fs, fp->id);					/* Check validity of the object */
   3529          	if (res != FR_OK) LEAVE_FF(fp->fs, res);
   3530          	if (fp->flag & FA__ERROR)						/* Check error flag */
   3531          		LEAVE_FF(fp->fs, FR_INT_ERR);
   3532          	if (!(fp->flag & FA_READ))						/* Check access mode */
   3533          		LEAVE_FF(fp->fs, FR_DENIED);
   3534          
   3535          	remain = fp->fsize - fp->fptr;
   3536          	if (btr > remain) btr = (UINT)remain;			/* Truncate btr by remaining bytes */
   3537          
   3538          	for ( ;  btr && (*func)(0, 0);					/* Repeat until all data transferred or stream becomes busy */
   3539          		fp->fptr += rcnt, *bf += rcnt, btr -= rcnt) {
   3540          		csect = (BYTE)(fp->fptr / SS(fp->fs) & (fp->fs->csize - 1));	/* Sector offset in the cluster */
   3541          		if ((fp->fptr % SS(fp->fs)) == 0) {			/* On the sector boundary? */
   3542          			if (!csect) {							/* On the cluster boundary? */
   3543          				clst = (fp->fptr == 0) ?			/* On the top of the file? */
   3544          					fp->sclust : get_fat(fp->fs, fp->clust);
   3545          				if (clst <= 1) ABORT(fp->fs, FR_INT_ERR);
   3546          				if (clst == 0xFFFFFFFF) ABORT(fp->fs, FR_DISK_ERR);
   3547          				fp->clust = clst;					/* Update current cluster */
   3548          			}
   3549          		}
   3550          		sect = clust2sect(fp->fs, fp->clust);		/* Get current data sector */
   3551          		if (!sect) ABORT(fp->fs, FR_INT_ERR);
   3552          		sect += csect;
   3553          		if (move_window(fp->fs, sect))				/* Move sector window */
   3554          			ABORT(fp->fs, FR_DISK_ERR);
   3555          		fp->dsect = sect;
   3556          		rcnt = SS(fp->fs) - (WORD)(fp->fptr % SS(fp->fs));	/* Forward data from sector window */
   3557          		if (rcnt > btr) rcnt = btr;
   3558          		rcnt = (*func)(&fp->fs->win[(WORD)fp->fptr % SS(fp->fs)], rcnt);
   3559          		if (!rcnt) ABORT(fp->fs, FR_INT_ERR);
   3560          	}
   3561          
   3562          	LEAVE_FF(fp->fs, FR_OK);
   3563          }
   3564          #endif /* _USE_FORWARD */
   3565          
   3566          
   3567          
   3568          #if _USE_MKFS && !_FS_READONLY
   3569          /*-----------------------------------------------------------------------*/
   3570          /* Create File System on the Drive                                       */
   3571          /*-----------------------------------------------------------------------*/
   3572          #define N_ROOTDIR	512		/* Number of root dir entries for FAT12/16 */
   3573          #define N_FATS		1		/* Number of FAT copies (1 or 2) */
   3574          
   3575          
   3576          FRESULT f_mkfs (
   3577          	BYTE drv,		/* Logical drive number */
   3578          	BYTE sfd,		/* Partitioning rule 0:FDISK, 1:SFD */
   3579          	UINT au			/* Allocation unit size [bytes] */
   3580          )
   3581          {
   3582          	static const WORD vst[] = { 1024,   512,  256,  128,   64,    32,   16,    8,    4,    2,   0};
   3583          	static const WORD cst[] = {32768, 16384, 8192, 4096, 2048, 16384, 8192, 4096, 2048, 1024, 512};
   3584          	BYTE fmt, md, *tbl;
   3585          	DWORD n_clst, vs, n, wsect;
   3586          	UINT i;
   3587          	DWORD b_vol, b_fat, b_dir, b_data;	/* Offset (LBA) */
   3588          	DWORD n_vol, n_rsv, n_fat, n_dir;	/* Size */
   3589          	FATFS *fs;
   3590          	DSTATUS stat;
   3591          
   3592          
   3593          	/* Check mounted drive and clear work area */
   3594          	if (drv >= _VOLUMES) return FR_INVALID_DRIVE;
   3595          	fs = FatFs[drv];
   3596          	if (!fs) return FR_NOT_ENABLED;
   3597          	fs->fs_type = 0;
   3598          	drv = LD2PD(drv);
   3599          
   3600          	/* Get disk statics */
   3601          	stat = disk_initialize(drv);
   3602          	if (stat & STA_NOINIT) return FR_NOT_READY;
   3603          	if (stat & STA_PROTECT) return FR_WRITE_PROTECTED;
   3604          #if _MAX_SS != 512					/* Get disk sector size */
   3605          	if (disk_ioctl(drv, GET_SECTOR_SIZE, &SS(fs)) != RES_OK)
   3606          		return FR_DISK_ERR;
   3607          #endif
   3608          	if (disk_ioctl(drv, GET_SECTOR_COUNT, &n_vol) != RES_OK || n_vol < 128)
   3609          		return FR_DISK_ERR;
   3610          	b_vol = (sfd) ? 0 : 63;	/* Volume start sector */
   3611          	n_vol -= b_vol;
   3612          	if (au & (au - 1)) au = 0;	/* Check validity of the AU size */
   3613          	if (!au) {					/* AU auto selection */
   3614          		vs = n_vol / (2000 / (SS(fs) / 512));
   3615          		for (i = 0; vs < vst[i]; i++) ;
   3616          		au = cst[i];
   3617          	}
   3618          	au /= SS(fs);		/* Number of sectors per cluster */
   3619          	if (au == 0) au = 1;
   3620          	if (au > 128) au = 128;
   3621          
   3622          	/* Pre-compute number of clusters and FAT syb-type */
   3623          	n_clst = n_vol / au;
   3624          	fmt = FS_FAT12;
   3625          	if (n_clst >= MIN_FAT16) fmt = FS_FAT16;
   3626          	if (n_clst >= MIN_FAT32) fmt = FS_FAT32;
   3627          
   3628          	/* Determine offset and size of FAT structure */
   3629          	if (fmt == FS_FAT32) {
   3630          		n_fat = ((n_clst * 4) + 8 + SS(fs) - 1) / SS(fs);
   3631          		n_rsv = 32;
   3632          		n_dir = 0;
   3633          	} else {
   3634          		n_fat = (fmt == FS_FAT12) ? (n_clst * 3 + 1) / 2 + 3 : (n_clst * 2) + 4;
   3635          		n_fat = (n_fat + SS(fs) - 1) / SS(fs);
   3636          		n_rsv = 1;
   3637          		n_dir = (DWORD)N_ROOTDIR * SZ_DIR / SS(fs);
   3638          	}
   3639          	b_fat = b_vol + n_rsv;				/* FAT area start sector */
   3640          	b_dir = b_fat + n_fat * N_FATS;		/* Directory area start sector */
   3641          	b_data = b_dir + n_dir;				/* Data area start sector */
   3642          	if (n_vol < b_data + au) return FR_MKFS_ABORTED;	/* Too small volume */
   3643          
   3644          	/* Align data start sector to erase block boundary (for flash memory media) */
   3645          	if (disk_ioctl(drv, GET_BLOCK_SIZE, &n) != RES_OK || !n || n > 32768) n = 1;
   3646          	n = (b_data + n - 1) & ~(n - 1);	/* Next nearest erase block from current data start */
   3647          	n = (n - b_data) / N_FATS;
   3648          	if (fmt == FS_FAT32) {		/* FAT32: Move FAT offset */
   3649          		n_rsv += n;
   3650          		b_fat += n;
   3651          	} else {					/* FAT12/16: Expand FAT size */
   3652          		n_fat += n;
   3653          	}
   3654          
   3655          	/* Determine number of clusters and final check of validity of the FAT sub-type */
   3656          	n_clst = (n_vol - n_rsv - n_fat * N_FATS - n_dir) / au;
   3657          	if (   (fmt == FS_FAT16 && n_clst < MIN_FAT16)
   3658          		|| (fmt == FS_FAT32 && n_clst < MIN_FAT32))
   3659          		return FR_MKFS_ABORTED;
   3660          
   3661          	/* Create partition table if required */
   3662          	if (sfd) {	/* No patition table (SFD) */
   3663          		md = 0xF0;
   3664          	} else {	/* With patition table (FDISK) */
   3665          		DWORD n_disk = b_vol + n_vol;
   3666          
   3667          		mem_set(fs->win, 0, SS(fs));
   3668          		tbl = fs->win+MBR_Table;
   3669          		ST_DWORD(tbl, 0x00010180);			/* Partition start in CHS */
   3670          		if (n_disk < 63UL * 255 * 1024) {	/* Partition end in CHS */
   3671          			n_disk = n_disk / 63 / 255;
   3672          			tbl[7] = (BYTE)n_disk;
   3673          			tbl[6] = (BYTE)((n_disk >> 2) | 63);
   3674          		} else {
   3675          			ST_WORD(&tbl[6], 0xFFFF);	/* CHS saturated */
   3676          		}
   3677          		tbl[5] = 254;
   3678          		if (fmt != FS_FAT32)				/* System ID */
   3679          			tbl[4] = (n_vol < 0x10000) ? 0x04 : 0x06;
   3680          		else
   3681          			tbl[4] = 0x0c;
   3682          		ST_DWORD(tbl+8, 63);				/* Partition start in LBA */
   3683          		ST_DWORD(tbl+12, n_vol);			/* Partition size in LBA */
   3684          		ST_WORD(fs->win+BS_55AA, 0xAA55);	/* MBR signature */
   3685          		if (disk_write(drv, fs->win, 0, 1) != RES_OK)	/* Put the MBR into first physical sector */
   3686          			return FR_DISK_ERR;
   3687          		md = 0xF8;
   3688          	}
   3689          
   3690          	/* Create volume boot record */
   3691          	tbl = fs->win;							/* Clear sector */
   3692          	mem_set(tbl, 0, SS(fs));
   3693          	mem_cpy(tbl, "\xEB\xFE\x90" "MSDOS5.0", 11);/* Boot jump code, OEM name */
   3694          	i = SS(fs);								/* Sector size */
   3695          	ST_WORD(tbl+BPB_BytsPerSec, i);
   3696          	tbl[BPB_SecPerClus] = (BYTE)au;			/* Sectors per cluster */
   3697          	ST_WORD(tbl+BPB_RsvdSecCnt, n_rsv);		/* Reserved sectors */
   3698          	tbl[BPB_NumFATs] = N_FATS;				/* Number of FATs */
   3699          	i = (fmt == FS_FAT32) ? 0 : N_ROOTDIR;	/* Number of rootdir entries */
   3700          	ST_WORD(tbl+BPB_RootEntCnt, i);
   3701          	if (n_vol < 0x10000) {					/* Number of total sectors */
   3702          		ST_WORD(tbl+BPB_TotSec16, n_vol);
   3703          	} else {
   3704          		ST_DWORD(tbl+BPB_TotSec32, n_vol);
   3705          	}
   3706          	tbl[BPB_Media] = md;					/* Media descriptor */
   3707          	ST_WORD(tbl+BPB_SecPerTrk, 63);			/* Number of sectors per track */
   3708          	ST_WORD(tbl+BPB_NumHeads, 255);			/* Number of heads */
   3709          	ST_DWORD(tbl+BPB_HiddSec, b_vol);		/* Hidden sectors */
   3710          	n = get_fattime();						/* Use current time as VSN */
   3711          	if (fmt == FS_FAT32) {
   3712          		ST_DWORD(tbl+BS_VolID32, n);		/* VSN */
