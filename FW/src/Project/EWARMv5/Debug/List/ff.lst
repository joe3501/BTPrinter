    519          /* Copy memory to memory */
    520          static
    521          void mem_cpy (void* dst, const void* src, UINT cnt) {
    522          	BYTE *d = (BYTE*)dst;
    523          	const BYTE *s = (const BYTE*)src;
    524          
    525          #if _WORD_ACCESS == 1
    526          	if (((unsigned)d & 0x03)!=0 || ((unsigned)s & 0x03)!=0)
    527          	{
    528          		while (cnt--)
    529          			*d++ = *s++;
    530          		return;
    531          	}
    532              
    533          	while (cnt >= sizeof(int)) {
    534          		*(int*)d = *(int*)s;
    535          		d += sizeof(int); s += sizeof(int);
    536          		cnt -= sizeof(int);
    537          	}
    538          #endif
    539          	while (cnt--)
    540          		*d++ = *s++;
    541          }
    542          
    543          /* Fill memory */
    544          static
    545          void mem_set (void* dst, int val, UINT cnt) {
    546          	BYTE *d = (BYTE*)dst;
    547          
    548          	while (cnt--)
    549          		*d++ = (BYTE)val;
    550          }
    551          
    552          /* Compare memory to memory */
    553          static
    554          int mem_cmp (const void* dst, const void* src, UINT cnt) {
    555          	const BYTE *d = (const BYTE *)dst, *s = (const BYTE *)src;
    556          	int r = 0;
    557          
    558          	while (cnt-- && (r = *d++ - *s++) == 0) ;
    559          	return r;
    560          }
