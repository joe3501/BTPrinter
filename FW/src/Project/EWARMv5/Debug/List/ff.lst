   3038          FRESULT f_seekdir(
   3039          	DIR *dj,		/* Pointer to the open directory object */
   3040          	int offset		/* the seek offset */
   3041          )
   3042          {
   3043          	int i = 0;
   3044          
   3045          	if (dir_sdi(dj, 0) != FR_OK || offset < 0)
   3046          		return FR_INT_ERR;
   3047          
   3048          	while(i < offset)
   3049          	{
   3050          		if(dir_read(dj) != FR_OK || dir_next(dj, 0) != FR_OK)
   3051          			return FR_INT_ERR;
   3052          		i++;
   3053          	} 
   3054          	return FR_OK;
   3055          }
