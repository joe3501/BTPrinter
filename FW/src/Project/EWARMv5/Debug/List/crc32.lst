     72          /* Return a 32-bit CRC of the contents of the buffer. */
     73          
     74          unsigned long crc32 (unsigned long crc, unsigned char *buf, int len)
     75          {
     76          	unsigned char *end;
     77          	
     78          	crc = ~crc;
     79          	for (end = buf + len; buf < end; ++buf)
     80          		crc = crc32_tab[(crc ^ *buf) & 0xff] ^ (crc >> 8);
     81          	return ~crc;
     82          }
