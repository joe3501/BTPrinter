    802              tmpsr = FSMC_Bank4->SR4;
    803            } 
    804            
    805            itstatus = tmpsr & FSMC_IT;
    806            
    807            itenable = tmpsr & (FSMC_IT >> 3);
    808          
    809            if ((itstatus != (u32)RESET)  && (itenable != (u32)RESET))
    810            {
    811              bitstatus = SET;
    812            }
    813            else
    814            {
    815              bitstatus = RESET;
    816            }
    817            return bitstatus; 
    818          }
