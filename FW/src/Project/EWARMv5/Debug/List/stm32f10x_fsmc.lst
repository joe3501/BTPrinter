    781          *******************************************************************************/ 
    782          ITStatus FSMC_GetITStatus(u32 FSMC_Bank, u32 FSMC_IT)
    783          {
    784            ITStatus bitstatus = RESET;
    785            u32 tmpsr = 0x0, itstatus = 0x0, itenable = 0x0; 
